{
  "name": "dquack",
  "version": "1.0.0",
  "description": "Quickly provision services utilizing docker",
  "private": true,
  "author": "pavlos noulis <pavlos.noulis@gmail.com>",
  "repository": "github:pnoulis/dquack",
  "homepage": "https://github.com/pnoulis/dquack#readme",
  "bugs": {
    "url": "https://github.com/pnoulis/dquack/issues"
  },
  "license": "GNU-3.0+",
  "keywords": [],
  "type": "module",
  "browserslist": [
    "current node"
  ],
  "scripts": {
    "serve": "source ./scripts/dotenv.sh && node ./build/main.bundle.cjs",
    "client": "source ./scripts/dotenv.sh && ./tests/routes.sh 2>&1",
    "build": "source ./scripts/dotenv.sh && webpack",
    "build:watch": "source ./scripts/dotenv.sh && webpack --watch",
    "build:run": "npm-run-all build -p serve 'client -- 1' --race"
  },
  "devDependencies": {
    "browserslist": "^4.21.3",
    "copy-webpack-plugin": "^11.0.0",
    "koa-json": "^2.0.2",
    "nodemon-webpack-plugin": "^4.8.1",
    "npm-run-all": "^4.1.5",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "@koa/router": "^12.0.0",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0"
  }
}
